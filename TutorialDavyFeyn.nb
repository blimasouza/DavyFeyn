Cell[CellGroupData[{

Cell["Example 1 - Massive Complex Scalar: JJ correlator", "Section",
 CellChangeTimes->{{3.667027883110249*^9, 3.6670278864446*^9}, {
  3.667034449598585*^9, 3.667034454886924*^9}, {3.6679116521597023`*^9, 
  3.66791167948936*^9}, {3.6689650895439453`*^9, 3.668965091674889*^9}}],

Cell["\<\
As our first example we will consider the theory of a free massive complex \
scalar in 3d. Its Lagrangian is given by\
\>", "Text",
 Editable->False,
 CellChangeTimes->{{3.6670278901018877`*^9, 3.667027936968186*^9}, {
   3.6670281362848186`*^9, 3.667028163264908*^9}, 3.667128287402266*^9}],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalL]", "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"\[LeftBracketingBar]", 
       RowBox[{"\[PartialD]", "\[Phi]"}], "\[RightBracketingBar]"}], "2"], 
     "-", 
     RowBox[{
      SuperscriptBox["m", "2"], 
      SuperscriptBox[
       RowBox[{"\[LeftBracketingBar]", "\[Phi]", "\[RightBracketingBar]"}], 
       "2"]}]}]}], TraditionalForm]],
  Editable->False],
 "."
}], "Text",
 CellChangeTimes->{{3.6670281682062597`*^9, 3.6670282332727137`*^9}, {
  3.668683680389422*^9, 3.668683701802527*^9}, {3.669019435905398*^9, 
  3.6690194626467113`*^9}},
 TextAlignment->Center],

Cell["\<\
The conserved current associated to the U(1) global symmetry of this model is\
\
\>", "Text",
 Editable->False,
 CellChangeTimes->{{3.6670282438221483`*^9, 3.667028290791677*^9}, {
  3.669018951271926*^9, 3.669018952960067*^9}}],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["J", "\[Mu]"], " ", "=", " ", 
    RowBox[{"\[ImaginaryI]", "(", 
     RowBox[{
      RowBox[{
       OverscriptBox["\[Phi]", "_"], 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Mu]"], "\[Phi]"}]}], " ", "-", " ", 
      RowBox[{"\[Phi]", 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Mu]"], 
        OverscriptBox["\[Phi]", "_"]}]}]}], ")"}]}], TraditionalForm]]],
 "."
}], "Text",
 Editable->False,
 CellChangeTimes->{{3.667028294633757*^9, 3.667028376551084*^9}},
 TextAlignment->Center],

Cell[TextData[{
 "We would like to compute the 2-point function of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["J", "\[Mu]"], TraditionalForm]]],
 ", i.e."
}], "Text",
 Editable->False,
 CellChangeTimes->{{3.6670284051209927`*^9, 3.667028428005253*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     RowBox[{
      SubscriptBox["J", "\[Mu]"], "(", 
      SubscriptBox["k", "1"], ")"}], 
     RowBox[{
      SubscriptBox["J", "\[Nu]"], "(", 
      RowBox[{"-", 
       SubscriptBox["k", "1"]}], ")"}]}], "\[RightAngleBracket]"}], " ", "=", 
   " ", 
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["\[DifferentialD]", "3"], "p"}], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"2", "\[Pi]"}], ")"}], "3"]], 
      SubscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "p"}], "+", 
         SubscriptBox["k", "1"]}], ")"}], "\[Mu]"], 
      FractionBox["\[ImaginaryI]", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["p", "2"], "-", 
         SuperscriptBox["m", "2"]}], ")"}]], 
      SubscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "p"}], "+", 
